{
  "name": "leadFundMobile",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "",
  "private": true,
  "scripts": {
    "dev": "bnr dev",
    "build": "bnr build",
    "dll": "bnr dll",
    "lint": "bnr lint",
    "fix": "bnr fix"
  },
  "betterScripts": {
    "dev": {
      "command": "node dev-server.js",
      "env": {
        "DEV_ENV": "dev"
      }
    },
    "build": {
      "command": "node build.js",
      "env": {
        "DEV_ENV": "build"
      }
    },
    "dll": {
      "command": "node webpack.dll.config.js"
    },
    "lint": {
      "command": "eslint --ext .js,.vue src/"
    },
    "fix": {
      "command": "eslint --fix .js,.vue src/components/**/* src/js/* src/router/* src/store/**/*"
    }
  },
  "dependencies": {
    "axios": "^0.16.2",
    "better-scroll": "^1.4.2",
    "md5": "^2.2.1",
    "vue": "^2.5.2",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
